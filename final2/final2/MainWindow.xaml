            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="63" Margin="734,374,0,0" VerticalAlignment="Top" Width="74">
            <Button.Background>
                <ImageBrush ImageSource="soup.jpg"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="63" Margin="813,374,0,0" VerticalAlignment="Top" Width="74">
            <Button.Background>
                <ImageBrush ImageSource="dessert.jpg"/>
            </Button.Background>
        </Button>
        <Image HorizontalAlignment="Left" Height="31" Margin="679,254,0,0" VerticalAlignment="Top" Width="38" RenderTransformOrigin="0.5,0.5">
            <Image.OpacityMask>
                <ImageBrush ImageSource="click.jpg"/>
            </Image.OpacityMask>
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-28.217"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Image HorizontalAlignment="Left" Height="24" Margin="725,94,0,0" VerticalAlignment="Top" Width="29">
            <Image.OpacityMask>
                <ImageBrush ImageSource="crown.jpg"/>
            </Image.OpacityMask>
        </Image>
    </Grid>
</Window>
