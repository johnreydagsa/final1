        <TextBlock HorizontalAlignment="Left" Height="19" Margin="418,442,0,0" TextWrapping="Wrap" Text="Healthy" VerticalAlignment="Top" Width="74" TextAlignment="Center" FontFamily="Sitka Heading" Foreground="#FF4398E2"/>
        <TextBlock HorizontalAlignment="Left" Height="19" Margin="497,442,0,0" TextWrapping="Wrap" Text="Salads" VerticalAlignment="Top" Width="74" TextAlignment="Center" FontFamily="Sitka Heading" Foreground="#FF4398E2"/>
        <TextBlock HorizontalAlignment="Left" Height="19" Margin="576,442,0,0" TextWrapping="Wrap" Text="Pastries" VerticalAlignment="Top" Width="74" TextAlignment="Center" FontFamily="Sitka Heading" Foreground="#FF4398E2"/>
