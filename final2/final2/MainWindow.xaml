                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
            </TextBox.Resources>
        </TextBox>
        <Label Content="Browse By Category" HorizontalAlignment="Left" Height="35" Margin="539,339,0,0" VerticalAlignment="Top" Width="215" FontFamily="Cooper Black" FontSize="20" Foreground="#FF4398E2"/>
