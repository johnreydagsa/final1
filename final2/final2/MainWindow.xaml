            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
            </Button.Resources>
        </Button>
        <TextBox HorizontalAlignment="Left" Height="25" Margin="539,300,0,0" TextWrapping="Wrap" Text="Or search for a specific recipe here!" VerticalAlignment="Top" Width="228" TextAlignment="Center" TextChanged="TextBox_TextChanged" FontFamily="Sitka Heading" FontSize="14" Foreground="#FF4398E2">
