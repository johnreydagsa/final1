        <TextBlock HorizontalAlignment="Left" Height="19" Margin="813,442,0,0" TextWrapping="Wrap" Text="Desserts" VerticalAlignment="Top" Width="74" TextAlignment="Center" FontFamily="Sitka Heading" Foreground="#FF4398E2"/>
        <Button Content="" HorizontalAlignment="Left" Height="57" Margin="610,208,0,0" VerticalAlignment="Top" Width="80">
            <Button.Background>
                <ImageBrush ImageSource="explore.jpg"/>
            </Button.Background>
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
            </Button.Resources>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="63" Margin="418,374,0,0" VerticalAlignment="Top" Width="74">
            <Button.Background>
                <ImageBrush ImageSource="healthy.jpg"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="63" Margin="497,374,0,0" VerticalAlignment="Top" Width="74" Click="Button_Click_1">
            <Button.Background>
                <ImageBrush ImageSource="salad.jpg"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="63" Margin="576,374,0,0" VerticalAlignment="Top" Width="74">
            <Button.Background>
                <ImageBrush ImageSource="pastry.jpg"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="63" Margin="655,374,0,0" VerticalAlignment="Top" Width="74">
