                            <Image Height ="80" Source="strawberry-lemonade-20.jpg" Stretch="Fill" DockPanel.Dock="Top" />
                            <Button Height ="25" DockPanel.Dock="Bottom" Background="White" BorderBrush="White" FontFamily="Sitka Heading">
                                <Image Height="20" Source="star rate.jpg" Stretch="Fill" Width="150"/>
                            </Button>
                            <Button  Background="White" Content="Strawberry Lemonade" BorderBrush="White" FontFamily="Sitka Heading" Margin="0,0.2,0,-0.2"/>
