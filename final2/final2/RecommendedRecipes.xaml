                        <TextBlock  Width="30" Text="&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;." FontSize="72" Foreground="Red"/>
                        <DockPanel Height="125" Width="150" >
                            <Image Height ="80" Source="vegesoup.jpg" Stretch="Fill" DockPanel.Dock="Top" />
                            <Button Height ="25" DockPanel.Dock="Bottom" Background="White" BorderBrush="White" FontFamily="Sitka Heading">
                                <Image Height="20" Source="star rate.jpg" Stretch="Fill" Width="150"/>
                            </Button>
                            <Button  Background="White" Content="Vegetable Soup" BorderBrush="White" FontFamily="Sitka Heading" Margin="0,0.2,0,-0.2"/>
                        </DockPanel>
                        <Button Height="125" DockPanel.Dock="Right" Background="White" Content="When we're looking for a comforting meal packed with &#xD;&#xA;vegetables, this easy veggie soup is where we turn. &#xD;&#xA;It's made with a light tomato broth and adapts to all seasons.&#xD;&#xA; Feel free to use any seasonal vegetables you have on hand." BorderBrush="White" Foreground="Black" FontFamily="Sitka Heading" FontSize="16"/>

